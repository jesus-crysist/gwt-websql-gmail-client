<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="Diplomski">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>

    <!-- Other module inherits                                      -->
    <inherits name="com.google.gwt.http.HTTP"/>
    <inherits name="com.google.gwt.json.JSON"/>
    <inherits name="com.google.code.gwt.database.Html5Database" />

    <!-- Specify the app entry point class.                   -->
    <entry-point class='edu.raf.jovica.diplomski.client.Diplomski'/>
    <servlet path="/Gmail" class="edu.raf.jovica.diplomski.server.rpc.GmailServiceImpl"/>

    <source path="client" />
    <source path='shared'/>

</module>