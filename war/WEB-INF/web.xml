<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <servlet>
        <servlet-name>edu.raf.jovica.diplomski.Diplomski Gmail</servlet-name>
        <servlet-class>edu.raf.jovica.diplomski.server.rpc.GmailServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>edu.raf.jovica.diplomski.Diplomski Gmail</servlet-name>
        <url-pattern>/Diplomski/Gmail</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>Diplomski.html</welcome-file>
    </welcome-file-list>
</web-app>
